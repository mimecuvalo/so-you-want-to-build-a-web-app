    "@mui/material": "7.2.0",
    "@vercel/analytics": "1.1.1",
    "circular-dependency-plugin": "5.2.2",
    "classnames": "2.5.1",
    "next": "15.3.5",
    "prism-react-renderer": "2.4.1",
    "prismjs": "1.30.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "sanitize.css": "13.0.0",
