    "@types/react": "19.1.8",
    "@types/react-dom": "19.1.6",
    "@types/uuid": "10.0.0",
    "@typescript-eslint/eslint-plugin": "8.36.0",
    "@typescript-eslint/parser": "8.36.0",
    "alex": "11.0.1",
    "axe-core": "4.10.3",
    "eslint": "9.30.1",
    "eslint-config-next": "15.3.5",
    "husky": "9.1.7",
    "import-sort-style-eslint": "6.0.0",
    "lint-staged": "16.1.2",
    "prettier": "3.6.2",
    "prettier-plugin-import-sort": "0.0.7",
    "ts-node": "10.9.2",
    "typescript": "5.8.3"
